{
  "name": "web",
  "private": true,
  "dependencies": {
    "bootstrap-sass": "~3.3.5",
    "modernizr": "~2.8.1",
    "roboto-fontface": "~0.4.3",
    "font-awesome": "~4.5.0",
    "swiper": "~3.3.1"
  },
  "overrides": {
